#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>
int main()
{
	int x, y, z, t;
	printf("Please enter three integers:");
	scanf("%d, %d, %d", &x, &y, &z);
	if (x > y)
	{
		t = x;
		x = y;
		y = t;
	}
	if (x > z)
	{
		t = x;
		x = z;
		z = t;
	}
	if (y > z)
	{
		t = y;
		y = z;
		z = t;
	}
	printf("The order of values from small to large is: %d ,%d ,%d", x, y, z);
	return 0;
}
