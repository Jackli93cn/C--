#define _CRT_SECURE_NO_WARNINGS 1

int main20()
{
	int a = 0;
	int b = 0;
	scanf("%d%d", &a, &b);
	int max = 0;
	if (a > b)
	{
		max = a;
	}
	else if (a < b)
	{
		max = b;
	}
	while (1)
	{
		if (a % max == 0 && b % max == 0)
		{
			printf("The greatest common divisor is:%d\n", max);
			break;
		}
		max--;
	}
	return 0;
}
